		<artifactId>migrationtool-ext-microservice</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
	<artifactId>migrationtool-ext-servicecutter</artifactId>
	<packaging>pom</packaging>
	<modules>
		<module>migrationtool-ext-servicecutter-core</module>
		<module>migrationtool-ext-servicecutter-api</module>
		<module>migrationtool-ext-servicecutter-gui</module>
		<module>migrationtool-servicecutter</module>
	</modules>

	<properties>
