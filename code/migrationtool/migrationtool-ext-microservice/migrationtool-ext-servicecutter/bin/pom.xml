				<artifactId>migrationtool-ext-servicecutter-gui</artifactId>
				<version>${servicecutter-gui-version}</version>
			</dependency>

			<dependency>
				<groupId>org.graphstream</groupId>
				<artifactId>gs-core</artifactId>
				<version>${graphstream-version}</version>
			</dependency>

			<dependency>
				<groupId>org.graphstream</groupId>
				<artifactId>gs-algo</artifactId>
				<version>${graphstream-version}</version>
			</dependency>
